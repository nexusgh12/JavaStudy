package javaProject2;

public class Magazine extends Book {

	private int mgnMonth;
	Magazine(String bookNo, String bookTitle, String bookAuthor, int bookYear, int bookPrice, String bookPublisher, int mgnMonth) {
		super(bookNo, bookTitle, bookAuthor, bookYear, bookPrice, bookPublisher);
		this.mgnMonth = mgnMonth;
	}
	
	public String toString() {
		return String.format("%s\t%s\t%s\t%d\t%d\t%s\t%d\t",getBookNo(), getBookTitle(), getBookAuthor(), getBookYear(), getBookPrice(), getBookPublisher(), getMgnMonth());
	}
	
	
	public void setMgnMonth(int mgnMonth) {
		this.mgnMonth = mgnMonth;
	}
	
	public int getMgnMonth() {
		return mgnMonth;
	}

}
